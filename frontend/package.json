{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "vitest --config vitest.config.storybook.ts",
    "test": "vitest --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts --watch",
    "test:coverage": "vitest --config vitest.config.ts --coverage",
    "test:ui": "vitest --config vitest.config.ts --ui",
    "test:playwright": "playwright test",
    "test:playwright:update": "playwright test --update-snapshots",
    "test:playwright:storybook": "playwright test -c playwright.storybook.config.ts",
    "test:playwright:storybook:update": "playwright test -c playwright.storybook.config.ts --update-snapshots",
    "test:playwright:storybook:dev": "PLAYWRIGHT_STORYBOOK_MODE=dev playwright test -c playwright.storybook.config.ts",
    "test:playwright:storybook:dev:update": "PLAYWRIGHT_STORYBOOK_MODE=dev playwright test -c playwright.storybook.config.ts --update-snapshots",
    "test:playwright:ui": "playwright test --ui",
    "generate:api": "openapi-ts",
    "postinstall": "node scripts/postinstall-playwright.js"
  },
  "dependencies": {
    "@hey-api/client-axios": "^0.2.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.475.0",
    "next": "^15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.35.0",
    "@playwright/test": "^1.55.1",
    "@storybook/addon-docs": "10.1.2",
    "@storybook/addon-onboarding": "10.1.2",
    "@storybook/addon-themes": "10.1.2",
    "@storybook/addon-vitest": "10.1.2",
    "@storybook/nextjs-vite": "10.1.2",
    "@types/node": "^20.11.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitest/browser": "4.0.14",
    "@vitest/browser-playwright": "4.0.14",
    "@vitest/coverage-v8": "4.0.14",
    "@vitest/runner": "4.0.14",
    "@vitest/ui": "4.0.14",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "playwright": "^1.55.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "storybook": "10.1.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "4.0.14"
  },
  "prettier": {
    "tabWidth": 4
  }
}
